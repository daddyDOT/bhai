<script defer type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/mathjax/2.7.9/MathJax.js?config=TeX-MML-AM_CHTML" nonce="3nMDT1bHLvFxdv15DxI9xYWfKxn3EkE9ufo7p/OxVMo=">
