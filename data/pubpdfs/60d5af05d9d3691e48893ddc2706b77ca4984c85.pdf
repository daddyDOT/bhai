<script type="text/javascript" nonce="3nMDT1bHLvFxdv15DxI9xYWfKxn3EkE9ufo7p/OxVMo=">
    // Have we already requested an update to the scrollspy this frame?
    let scrollSpyUpdateScheduled = false;

    documentSectionOffsets = [];
    scrollPointer = 0;
    documentHrefToSections = {};

    // click scroll spy, needs to be real href, so body scrolls
    // c# has the href and data-section generated values

    window.addEventListener('load', function(event) {
        this.mapScrollSpyHrefSections();
        this.bindScrollSpy();
        this.bindImagesToGallery();
        this.hideFiguresAddExpandButtons();
        this.handleReferencesClicks();
        this.capitaliseFigureIds();
        this.bindSuperscriptAnchorTags();
        this.addExLibrisButtonsToReferences();
    });

    window.addEventListener('scroll', tryScheduleScrollSpyUpdate);
    window.addEventListener('resize', tryScheduleScrollSpyUpdate);

    function tryScheduleScrollSpyUpdate() {
        // Don't request a scroll spy update if one is already scheduled to be performed this frame
        if (scrollSpyUpdateScheduled) 
            return;

        // We're requesting an update, so block any other events from doing the same this frame
        scrollSpyUpdateScheduled = true;
        // Queue a callback to be performed just before the browser paints the next frame
        requestAnimationFrame(UpdateScroll);
    }

    function capitaliseFigureIds() {
        let figureIdLooping = true;
        let figureIdIdx = 0;
        while (figureIdLooping) {
            figureIdIdx++;
            const lowercaseFigId = `f${figureIdIdx}`;
            const fig = document.querySelector(`#${lowercaseFigId}`);
            if (fig !== null) {
                fig.id = `F${figureIdIdx}`
                const figRefs = document.querySelectorAll(`a[data-target="${lowercaseFigId}"]`);
                figRefs.forEach((figRef) => {
                    figRef.href = `#F${figureIdIdx}`;
                    figRef.dataset.target = fig.id;
                    figRef.id = `#refF${figureIdIdx}`
                });
            } else {
                figureIdLooping = false;
            }
        }
    }

    function UpdateScroll() {
        // We're updating scroll spy for this frame, so free the schedule lock so the next frame can issue another update request
        scrollSpyUpdateScheduled = false;

        var y = window.scrollY + 2; // (+X to just slightly offset the scroll to select the right element)
        var index = 0;
        for (var i = 0; i < this.documentSectionOffsets.length; i++) {
            var section = this.documentSectionOffsets[i];
            if (y < section.offset) {
                continue;
            }

            index = i;
        }

        var selectSection = this.documentSectionOffsets[index];
        if (!selectSection)
            return;
        this.selectScrollSpySection(selectSection.id);
    }

    function mapScrollSpyHrefSections() {
        var navbar = document.getElementById('navbar-scrollspy');

        var aTags = navbar.getElementsByTagName('a');
        
        for (var i = 0; i < aTags.length; i++) {
            var aTag = aTags[i];

            var href = '#' + aTag.href.split('#')[1];
            var sectionId = aTag.getAttribute('data-section');

            this.documentHrefToSections[href] = sectionId;
        }
    }

    function bindScrollSpy() {
        var mainBody = document.querySelectorAll("#article-body")[0];
        var mainBodyOffset = mainBody.offsetTop;

        var sections = document.getElementsByTagName('section');
        sectionOffsets = [];

        for (var i = 0; i < sections.length; i++) {
            var section = sections[i];
            var id = section.id;
            var offset = section.offsetTop + mainBodyOffset;

            sectionOffsets.push({ id: '#' + id, offset: offset });
        }

        this.documentSectionOffsets = sectionOffsets;
        this.selectScrollSpySection(window.location.hash);
    }

    function selectScrollSpySection(hrefHash) {
        var hash = this.documentHrefToSections['' + hrefHash];

        if (!hash) {
            return;
        }

        var sectionHash = hash.split(/-|_/)[0];
        var sectionLabelHash = hash;

        var navbar = document.getElementById('navbar-scrollspy');
        var aTags = navbar.getElementsByTagName('a');

        // Active headings
        var activeSection = navbar.querySelector("a[data-section='" + sectionLabelHash + "']");
        var inactiveSections = navbar.querySelectorAll("a:not([data-section='" + sectionLabelHash + "'])");

        if (activeSection) {
            activeSection.classList.add('section-highlight');
        }
        inactiveSections.forEach(section => section.classList.remove('section-highlight'));
        
        // Active main headings
        activeSection = navbar.querySelector("a[data-section='" + sectionHash + "']");
        inactiveSections = navbar.querySelectorAll("a:not([data-section='" + sectionHash + "'])");

        if (activeSection) {
            activeSection.classList.add('section-lighter-highlight');
        }
        inactiveSections.forEach(section => section.classList.remove('section-lighter-highlight'));

        // Sections
        activeSection = navbar.querySelector("nav.sub-nav[data-section='" + sectionHash + "']");
        inactiveSections = navbar.querySelectorAll("nav.sub-nav:not([data-section='" + sectionHash + "'])");

        if (activeSection) {
            activeSection.classList.add('section-highlight');
        }
        inactiveSections.forEach(section => section.classList.remove('section-highlight'));
    }

    function bindImagesToGallery() {
        var elements = document.querySelectorAll("#article-body div[class='graphic']");

        for (var i = 0; i < elements.length; i++) {
            var imageElement = elements[i].getElementsByTagName("img")[0];

            imageElement.setAttribute('data-image-id', i);

            if (i > 0) {
                imageElement.setAttribute('data-prev-image-id', i - 1);
            }

            if (i != elements.length - 1) {
                imageElement.setAttribute('data-next-image-id', i + 1);
            }

            imageElement.addEventListener('click', function (e) {
                var targetElement = e.target;
                // Go up from img, to .graphic to .figure then back down to .figure-description
                var descriptionElement = targetElement.parentElement.parentElement.querySelector("div.figure-description");

                var src = targetElement.src;
                var prevImageId = targetElement.getAttribute('data-prev-image-id');
                var nextImageId = targetElement.getAttribute('data-next-image-id');

                var title = descriptionElement.getElementsByClassName('figure-label')[0];
                var description = descriptionElement.getElementsByClassName('figure-caption')[0];

                document.getElementById('image-gallery-img').setAttribute('src', src);
                document.getElementById('image-gallery-title').innerText = title.innerText;
                document.getElementById('image-gallery-description').innerText = description.innerText;

                var prevImageButton = document.getElementById('arrow-prev');
                var nextImageButton = document.getElementById('arrow-next');
                
                if (!prevImageId) {
                    prevImageButton.style.opacity = '0';
                    prevImageButton.removeAttribute('data-image-id');
                } else {
                    prevImageButton.style.opacity = '1';
                    prevImageButton.setAttribute('data-image-id', prevImageId);
                }

                if (!nextImageId) {
                    nextImageButton.style.opacity = '0';
                    nextImageButton.removeAttribute('data-image-id');
                } else {
                    nextImageButton.style.opacity = '1';
                    nextImageButton.setAttribute('data-image-id', nextImageId);
                }

                document.getElementById('image-gallery').style.removeProperty('display');
                document.getElementById('image-gallery').classList.add('show');
            });
        }

        var prevImageButton = document.getElementById('arrow-prev');
        prevImageButton.addEventListener('click', function(e) {
            var id = prevImageButton.getAttribute('data-image-id');
            document.querySelector('img[data-image-id="' + id + '"]').click();
        });

        var nextImageButton = document.getElementById('arrow-next');
        nextImageButton.addEventListener('click', function(e) {
            var id = nextImageButton.getAttribute('data-image-id');
            document.querySelector('img[data-image-id="' + id + '"]').click();
        });

        document.getElementById('image-gallery-close').addEventListener('click', function() {
            document.getElementById('image-gallery').classList.remove('show');
        });

        document.getElementById('image-gallery-download').addEventListener('click', function() {
            var src = document.getElementById('image-gallery-img').src;
            var title = document.getElementById('image-gallery-title').innerText;

            var link = document.createElement("a");
            link.setAttribute('download', title);
            link.href = src;
            document.body.appendChild(link);
            link.click();
            link.remove();
        });
    }

    function hideFiguresAddExpandButtons() {
        var figures = document.querySelectorAll('#article-body .figure');

        for (var figureIdx = 0; figureIdx < figures.length; figureIdx++) {
            var figure = figures[figureIdx];
            var graphic = figure.querySelectorAll('.graphic')[0];

            // Hide the figure by default
            figure.style.display = 'none';

            // Button in the article body to toggle figure visibility
            var expandButton = document.createElement('button');
            expandButton.classList.add('btn', 'btn-success', 'rounded-pill', 'bjp-bg-journal-colour', 'bjp-border-journal-colour', 'm-1', 'mb-3');
            expandButton.setAttribute('data-figure-id', figureIdx);

            const innerSpan = document.createElement('span');
            innerSpan.innerText = 'Show Figure ' + (figureIdx + 1);
            innerSpan.classList.add('me-2');
            expandButton.append(innerSpan);

            const arrowIcon = document.createElement('span');
            arrowIcon.classList.add("icon-mask", "icon-arrow-down", "icon-colour-white", "icon-size-75");
            expandButton.append(arrowIcon);

            expandButton.addEventListener('click', function(event) {
                const figureContainer = event.currentTarget.nextSibling;

                var isExpanded = figureContainer.style.display !== 'none';

                if (isExpanded) {
                    figureContainer.style.display = 'none';
                    arrowIcon.classList.remove("icon-arrow-up");
                    arrowIcon.classList.add("icon-arrow-down");
                    innerSpan.innerText = innerSpan.innerText.replace("Hide", "Show");
                } else {
                    figureContainer.style.display = 'block';
                    arrowIcon.classList.remove("icon-arrow-down");
                    arrowIcon.classList.add("icon-arrow-up");
                    innerSpan.innerText = innerSpan.innerText.replace("Show", "Hide");
                }
                
                setTimeout(function (){
                    bindScrollSpy();
                    UpdateScroll();
                }, 250);
            });

            graphic.parentNode.parentNode.insertBefore(expandButton, figure);

            // 'X' in the top right of the expanded figure container which closes the figure
            const crossContainer = document.createElement("div");
            crossContainer.classList.add("d-flex", "flex-row-reverse");

            const crossIcon = document.createElement("span");
            crossIcon.classList.add("icon-mask", "icon-cross", "icon-colour-gray", "icon-size-150", "mt-4", "me-4", "cursor-pointer");

            // Event listener which closes the figure upon clicking
            // Ensure the state of the toggle button icon is correct
            crossIcon.addEventListener("click", (event) => {
                const figureContainer = event.currentTarget.parentNode.parentNode.parentNode;
                figureContainer.style.display = 'none';
                arrowIcon.classList.remove("icon-arrow-up");
                arrowIcon.classList.add("icon-arrow-down");
                innerSpan.innerText = innerSpan.innerText.replace("Hide", "Show");
            });
            
            crossContainer.append(crossIcon);
            graphic.prepend(crossContainer);


            // Tip informing the user how to open the fullscreen viewer from an expanded figure
            const descriptionContainer = figure.querySelector(".figure-description");
            const descriptionLabel = descriptionContainer.querySelector(".figure-label");
            const labelText = descriptionLabel.firstChild.innerText

            const newLabelContainer = document.createElement("span");
            newLabelContainer.classList.add("d-flex", "justify-content-between");

            const openViewerTip = document.createElement("span");
            openViewerTip.classList.add("label");
            openViewerTip.setAttribute("style", "margin-top: 1.25rem; margin-bottom: .5rem; font-weight: 400; color: #6c757d;");
            openViewerTip.innerText = "Click image to open viewer";

            newLabelContainer.append(descriptionLabel.cloneNode(true));
            newLabelContainer.append(openViewerTip);

            // Remove original label as it's been cloned into the new container
            descriptionLabel.remove();

            descriptionContainer.prepend(newLabelContainer);

        }

        setTimeout(function (){
            bindScrollSpy();
            UpdateScroll();
        }, 250);
    }

    function handleReferencesClicks() {
        var links = document.querySelectorAll('.link-bibr');
        for (var i = 0; i < links.length; i++) {
            let link = links[i];
            link.addEventListener('click', (ev) => {
                var tab = document.querySelector('#btnReferences');
                bootstrap.Tab.getOrCreateInstance(tab).show();                
                tab.addEventListener('shown.bs.tab', function (event) { window.location = link.href; }, { once: true });
            });
        }
    }

    function bindSuperscriptAnchorTags() {
        var tags = document.querySelectorAll('sup a');

        tags.forEach(tag => {
            tag.addEventListener('click', element => {
                var href = element.target.href.split('#')[1];

                if (window.innerWidth < 576) {
                    var mobileReferencesTab = document.querySelector('a[data-baj-mobile-nav="#tabReferences"]');
                    mobileReferencesTab.click();
                    // Mobile seems to scroll automagically
                } else if (window.innerWidth < 992) {
                    var offCanvasReferencesTab = document.querySelector('a[href="#offcanvasReferences"]');
                    offCanvasReferencesTab.click();
                    // Table size seems to not, so scroll now
                    setTimeout(function () {
                        var tabletReferenceElement = document.querySelector(`#offcanvasReferencesBody [id="${href}"]`);

                        var tabletCanvasContent = document.getElementById('offcanvasReferencesBody');
                        var offset = tabletReferenceElement.offsetTop;
                        if (offset > 48) {
                            offset -= 48;
                        }
                        tabletCanvasContent.scrollTo(0, offset);
                    }, 350); // the .show class animation is 300ms, give a little time for the renderer
                }
            });
        });
    }

    function addExLibrisButtonsToReferences() {
        var isInstitutionalUser = false;
        if (!isInstitutionalUser) {
            return;
        }

        fetch('/api/asset/openurl')
            .then(response => {
                return response.json();
            })
            .then(json => {
                var refs = document.querySelectorAll('.ref-list .reference');

                var url = json.openUrl;
                // Is this a good enough check to know if this is an OpenURL link and not a referer
                if (!url.indexOf("openurl") == -1) {
                    return;
                }
                if (!url.endsWith("?")) {
                    url += "?";
                }

                refs.forEach(ref => {
                    var id = ref.id;

                    var articleTitle = ref.querySelector('.reference-article-title');
                    var doi = ref.querySelector('.doi');
                    if (doi != null) {
                        doi = new URL(doi.getAttribute('href')).pathname.substr(1);
                    }
                    var fPage = ref.querySelector('.reference-fpage');
                    var lPage = ref.querySelector('.reference-lpage');
                    var year = ref.querySelector('.reference-year');
                    var volume = ref.querySelector('.reference-volume');
                    var issue = ref.querySelector('.reference-issue');

                    var params = new URLSearchParams('');
                    if (articleTitle != null) {
                        params.append('rft.atitle', articleTitle.textContent);
                    }
                    if (doi != null) {
                        params.append('rft.doi', doi);
                        params.append('doi', doi);
                    }
                    if (fPage != null && lPage != null) {
                        params.append('rft.pages', `${fPage.textContent}-${lPage.textContent}`);
                    }
                    if (year != null) {
                        params.append('rft.pubyear', year.textContent);
                    }
                    if (volume != null) {
                        params.append('rft.volume', volume.textContent);
                    }
                    if (issue != null) {
                        params.append('rft.issue', issue.textContent);
                    }

                    // No entries, just ignore and move along
                    if (params.toString() == '') {
                        return;
                    }

                    var a = document.createElement('a');
                    a.href = `${url}${params}`;
                    a.classList = 'link link-external exLibris ms-1';
                    a.setAttribute('target', '_blank');

                    var img = document.createElement('img');
                    img.src = json.buttonLink;
                    a.appendChild(img);

                    ref.appendChild(a);

                    var sidebarReference = document.querySelector(`#tab-reference-${id}`);
                    if (sidebarReference != null) {
                        var externalLinks = sidebarReference.querySelector('.tab-external-links');
                        externalLinks.appendChild(a.cloneNode(true));
                    }
                });
            });
    }
</script>



